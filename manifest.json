{
  "runtime": "org.freedesktop.Platform//40",
  "runtime-version": "40",
  "sdk": "org.freedesktop.Sdk//40",
  "command": "python3",
  "modules": [
    {
      "name": "glosungci-cd",
      "buildsystem": "SConstruct",
      "build-options": {
        "env": {
          "PYTHON": "/usr/bin/python3",
          "FLATPAK_SDK": "/usr/share/sdk",
          "FLATPAK_RUNTIME": "/usr/share/runtime/org.freedesktop.Platform/x86_64/40",
          "FLATPAK_GL_DRIVERS": "/usr/lib/dri"
        }
      },
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/glosung/glosungCI-CD.git",
          "branch": "main"
        }
      ]
    }
  ]
}
